      <div class="card-body">
        <h5 class="card-title"><%= c.title %></h5>
        <p class="card-text">Chekout our <%= c.title%> section  </p>
        <a href="/products/<%= c.slug %>" class="btn btn-primary"><%= c.title %> section</a>
      </div>
    </div>


  </div>

  <% }); %>
</div>

